desc:EQ Native

filename:0,eq.png
filename:1,s1.png
filename:2,s2_1.png
filename:3,s2_2.png
filename:4,s2.png
filename:5,s2_3.png
filename:6,s2_4.png




slider1:150<0,1000,5>-HPF (Hz)
slider2:420<0,1000,1>-Band 1 (Hz)
slider3:0.5<.5,10,0.1>-Q 1
slider4:0<-12,6,0.5>-Gain 1 (dB)
slider5:900<900,6000,1>-Band 2 (Hz)
slider6:0.5<.5,10,0.1>-Q 2
slider7:0<-12,6,0.5>-Gain 2 (dB)
slider8:5000<5000,10000,1>-Band 3 (Hz)
slider9:0.5<.5,10,0.1>-Q 3
slider10:0<-12,6,0.5>-Gain 3 (dB)
slider11:9000<9000,22000,1>-Band 4 (Hz)
slider12:0.5<.5,10,0.1>-Q 4
slider13:0<-12,6,0.5>-Gain 4 (dB)
slider14:20000<10000,22000,5>-LPF (Hz)
slider15:0<-12,12,0.5>-Output Gain (dB)
slider16:70<0,100,1>-noparameter
slider17:0<-100,100,0.1>-Pan
slider18:0<-6,6,0.5>-Pan Width


in_pin:left input
in_pin:right input
out_pin:left output
out_pin:right output


@init
HOLDTIME = srate/128;

r1Timer = 0;
r2Timer = HOLDTIME/2;

gr_meter=1;
gr_meter_decay = exp(1/(1*srate));

pan=0.01*slider17;

cDcAdd = 10^-30; 
cDenorm = 10^-30;
  gfx_clear = 1

@slider

//pan

panlaw=exp(slider18*AMP_DB_i);
pancomp=(panlaw > 1.0 ? 1.0/panlaw : panlaw);
dpan=0.0;


//sat
foo=slider16/200*$pi;
bar = sin(slider16/230*$pi);

//EQ
  gain = 10^(slider15/20);

  a0 = 1;
  s0 = 1;
  q0 = 1 / (sqrt((a0 + 1/a0)*(1/s0 - 1) + 2));
  w00 = 2 * $pi * slider1/srate;
  cosw00 = cos(w00);
  sinw00 = sin(w00);
  alpha0 = sinw00 / (2 * q0);

  b00 = (1 + cosw00)/2;
  b10 = -(1 + cosw00);
  b20 = (1 + cosw00)/2;
  a00 = 1 + alpha0;
  a10 = -2 * cosw00;
  a20 = 1 - alpha0;
  b00 /= a00;
  b10 /= a00;
  b20 /= a00;
  a10 /= a00;
  a20 /= a00;

  a1 = (10^(slider4/40));
  q1 = slider3;
  w01 = 2 * $pi * slider2/srate;
  cosw01 = cos(w01);
  sinw01 = sin(w01);
  alpha1 = sinw01 / (2 * q1);

  b01 = 1 + alpha1 * a1;
  b11 = -2 * cosw01;
  b21 = 1 - alpha1 * a1;
  a01 = 1 + alpha1 / a1;
  a11 = -2 * cosw01;
  a21 = 1 - alpha1 / a1;
  b01 /= a01;
  b11 /= a01;
  b21 /= a01;
  a11 /= a01;
  a21 /= a01;

  a3 = (10^(slider7/40));
  q3 = slider6;
  w03 = 2 * $pi * slider5/srate;
  cosw03 = cos(w03);
  sinw03 = sin(w03);
  alpha3 = sinw03 / (2 * q3);

  b03 = 1 + alpha3 * a3;
  b13 = -2 * cosw03;
  b23 = 1 - alpha3 * a3;
  a03 = 1 + alpha3 / a3;
  a13 = -2 * cosw03;
  a23 = 1 - alpha3 / a3;
  b03 /= a03;
  b13 /= a03;
  b23 /= a03;
  a13 /= a03;
  a23 /= a03;

  a5 = (10^(slider10/40));
  q5 = slider9;
  w05 = 2 * $pi * slider8/srate;
  cosw05 = cos(w05);
  sinw05 = sin(w05);
  alpha5 = sinw05 / (2 * q5);

  b05 = 1 + alpha5 * a5;
  b15 = -2 * cosw05;
  b25 = 1 - alpha5 * a5;
  a05 = 1 + alpha5 / a5;
  a15 = -2 * cosw05;
  a25 = 1 - alpha5 / a5;
  b05 /= a05;
  b15 /= a05;
  b25 /= a05;
  a15 /= a05;
  a25 /= a05;

  a7 = (10^(slider13/40));
  q7 = slider12;
  w07 = 2 * $pi * slider11/srate;
  cosw07 = cos(w07);
  sinw07 = sin(w07);
  alpha7 = sinw07 / (2 * q7);

  b07 = 1 + alpha7 * a7;
  b17 = -2 * cosw07;
  b27 = 1 - alpha7 * a7;
  a07 = 1 + alpha7 / a7;
  a17 = -2 * cosw07;
  a27 = 1 - alpha7 / a7;
  b07 /= a07;
  b17 /= a07;
  b27 /= a07;
  a17 /= a07;
  a27 /= a07;


  a9 = 1;
  s9 = 2;
  q9 = 1 / (sqrt((a9 + 1/a9)*(1/s9 - 1) + 2));
  w09 = 2 * $pi * slider14/srate;
  cosw09 = cos(w09);
  sinw09 = sin(w09);
  alpha9 = sinw09 / (2 * q9);

  b09 = (1 - cosw09)/2;
  b19 = (1 - cosw09);
  b29 = (1 - cosw09)/2;
  a09 = 1 + alpha9;
  a19 = -2 * cosw09;
  a29 = 1 - alpha9;
  b09 /= a09;
  b19 /= a09;
  b29 /= a09;
  a19 /= a09;
  a29 /= a09;
 
 
//pan  
@block

dpan=0.0;

ddb=(tgtdb-db)/db_chg_splpos;

pan_chg_splpos=slider_next_chg(2, tgtpan);
pan_chg_splpos > 0 ? 
(
  pan=0.01*slider17;
) : (
  tgtpan = slider17;
  pan_chg_splpos = samplesblock;
);
dpan=(tgtpan*0.01-pan)/pan_chg_splpos;
  

@sample

//pan


cnt == pan_chg_splpos ? 
(
  dpan=0.0;
  pan_chg_splpos=slider_next_chg(2, tgtpan);
  pan_chg_splpos > cnt ? 
  (
    dpan=(tgtpan*0.01-pan)/(pan_chg_splpos-cnt);
  );
);
adj=exp(db*AMP_DB_i);
panlaw != 1.0 ? 
(
  panlaw > 1.0 ? adj *= panlaw;
  panatt = abs(pan);
  adj *= pancomp+(1.0-pancomp)*(2.0/(2.0-panatt)-1.0);
);

adj0=adj1=adj;
pan < 0.0 ? adj1 *= 1.0+pan;
pan > 0.0 ? adj0 *= 1.0-pan;

spl0 *= adj0;
spl1 *= adj1;

db += ddb;
pan += dpan;
cnt += 1;;
cnt += 1;



//sat
  slider16 ? (
  spl0 = min(max( sin(max(min(spl0,1),-1)*foo)/bar ,-1) ,1);
  spl1 = min(max( sin(max(min(spl1,1),-1)*foo)/bar ,-1) ,1);
  );

//Eq
  slider1 != 0 ? (
  ospl0 = spl0;
  spl0 = b00 * spl0 + b10 * xl10 + b20 * xl20 - a10 * yl10 - a20 * yl20;
  xl20 = xl10;
  xl10 = ospl0;
  yl20 = yl10;
  yl10 = abs(spl0) < cDenorm ? 0 : spl0;

  ospl1 = spl1;
  spl1 = b00 * spl1 + b10 * xr10 + b20 * xr20 - a10 * yr10 - a20 * yr20;
  xr20 = xr10;
  xr10 = ospl1;
  yr20 = yr10;
  yr10 = abs(spl1) < cDenorm ? 0 : spl1;
  );


  spl0 += cDcAdd;
  spl1 += cDcAdd;


  slider4 != 0 && slider2 != 0 ? (
  ospl0 = spl0;
  spl0 = b01 * spl0 + b11 * xl11 + b21 * xl21 - a11 * yl11 - a21 * yl21;
  xl21 = xl11;
  xl11 = ospl0;
  yl21 = yl11;
  yl11 = spl0;

  ospl1 = spl1;
  spl1 = b01 * spl1 + b11 * xr11 + b21 * xr21 - a11 * yr11 - a21 * yr21;
  xr21 = xr11;
  xr11 = ospl1;
  yr21 = yr11;
  yr11 = spl1;
  );

  slider7 && slider5 != 0 ? (
  ospl0 = spl0;
  spl0 = b03 * spl0 + b13 * xl13 + b23 * xl23 - a13 * yl13 - a23 * yl23;
  xl23 = xl13;
  xl13 = ospl0;
  yl23 = yl13;
  yl13 = spl0;

  ospl1 = spl1;
  spl1 = b03 * spl1 + b13 * xr13 + b23 * xr23 - a13 * yr13 - a23 * yr23;
  xr23 = xr13;
  xr13 = ospl1;
  yr23 = yr13;
  yr13 = spl1;
  );

  slider10 && slider8 != 0 ? (
  ospl0 = spl0;
  spl0 = b05 * spl0 + b15 * xl15 + b25 * xl25 - a15 * yl15 - a25 * yl25;
  xl25 = xl15;
  xl15 = ospl0;
  yl25 = yl15;
  yl15 = spl0;

  ospl1 = spl1;
  spl1 = b05 * spl1 + b15 * xr15 + b25 * xr25 - a15 * yr15 - a25 * yr25;
  xr25 = xr15;
  xr15 = ospl1;
  yr25 = yr15;
  yr15 = spl1;
  );

  slider13 && slider11 != 0 ? (
  ospl0 = spl0;
  spl0 = b07 * spl0 + b17 * xl17 + b27 * xl27 - a17 * yl17 - a27 * yl27;
  xl27 = xl17;
  xl17 = ospl0;
  yl27 = yl17;
  yl17 = spl0;

  ospl1 = spl1;
  spl1 = b07 * spl1 + b17 * xr17 + b27 * xr27 - a17 * yr17 - a27 * yr27;
  xr27 = xr17;
  xr17 = ospl1;
  yr27 = yr17;
  yr17 = spl1;
  );


  slider14 != 22000 ? (
  ospl0 = spl0;
  spl0 = b09 * spl0 + b19 * xl19 + b29 * xl29 - a19 * yl19 - a29 * yl29;
  xl29 = xl19;
  xl19 = ospl0;
  yl29 = yl19;
  yl19 = spl0;

  ospl1 = spl1;
  spl1 = b09 * spl1 + b19 * xr19 + b29 * xr29 - a19 * yr19 - a29 * yr29;
  xr29 = xr19;
  xr19 = ospl1;
  yr29 = yr19;
  yr19 = spl1;
  );
     

  spl0 *= gain;
  spl1 *= gain;

@gfx 940 231
 gfx_setfont(1,"Arial",14,'a');

mouse_cap > 0 ? (
  mouseon == 0 ? (
    mouseclick = 1;
    mouse_cap == 2 ? val=1.0 : val = 5;

            //pan
            (mouse_x>=857&&mouse_x<=908&&mouse_y>=125&&mouse_y<=190) ? (mouseon=17;mouseclick=17;);
            );
            
            
             //Pan
            
            mouseon == 17 ? (
             mouse_y < saveY ? slider17 = max(-100,slider17-val);
             mouse_y > saveY ? slider17 = min(100,slider17+val);
             mouse_cap == 2 ? slider17 = 0;
             
             );

    

mouse_cap > 0 ? (
  mouseon == 0 ? (
    mouseclick = 1;
    mouse_cap == 2 ? val=1.0 : val = 60;
    //HPF
    (mouse_x>=38&&mouse_x<=108&&mouse_y>=126&&mouse_y<=180) ? (mouseon=1;mouseclick=1;);
    //LPF 
    (mouse_x>=89&&mouse_x<=140&&mouse_y>=45&&mouse_y<=90) ?   (mouseon=14;mouseclick=250;);
    //Freq Low 1
    (mouse_x>=180&&mouse_x<=245&&mouse_y>=45&&mouse_y<=90) ? (mouseon=2;mouseclick=2;); 
    //Freq Low/Mid 2
    (mouse_x>=335&&mouse_x<=398&&mouse_y>=45&&mouse_y<=90) ? (mouseon=5;mouseclick=5;);
    //Freq Mid/High 3
    (mouse_x>=480&&mouse_x<=550&&mouse_y>=45&&mouse_y<=90) ? (mouseon=8;mouseclick=8;);
    //Freq High 4
    (mouse_x>=640&&mouse_x<=710&&mouse_y>=45&&mouse_y<=90) ? (mouseon=11;mouseclick=11;);
     
    mouse_cap == 5 ? val*=13.;
    mouse_cap == 17 ? val*=0.3;
    //HPF
     );
      mouseon == 1 ? (
        mouse_y < saveY ? slider1 = max(0,slider1-val);
        mouse_y > saveY ? slider1 = min(1000,slider1+val);
        mouse_cap == 2 ? slider1 = 150;
      );
    
    //LPF
     mouseon == 14 ? (
       mouse_y < saveY ? slider14 = max(10000,slider14-val);
       mouse_y > saveY ? slider14 = min(22000,slider14+val);
       mouse_cap == 2 ? slider14 = 20000;
     );
     
     
    //Freq Low 1
     mouseon == 2 ? (
      mouse_y < saveY ? slider2 = max(0,slider2-val);
      mouse_y > saveY ? slider2 = min(1000,slider2+val);
      mouse_cap == 2 ? slider2 = 420;
      
      );
      
      //Freq Low/Mid 2
       mouseon == 5 ? (
        mouse_y < saveY ? slider5 = max(900,slider5-val);
        mouse_y > saveY ? slider5 = min(6000,slider5+val);
        mouse_cap == 2 ? slider5 = 900;
        
        );
        
        //Freq Mid/high 3
         mouseon == 8 ? (
          mouse_y < saveY ? slider8 = max(5000,slider8-val);
          mouse_y > saveY ? slider8 = min(10000,slider8+val);
          mouse_cap == 2 ? slider8 = 5000;
          
          );
          
          //Freq high 4
           mouseon == 11 ? (
            mouse_y < saveY ? slider11 = max(9000,slider11-val);
            mouse_y > saveY ? slider11 = min(22000,slider11+val);
            mouse_cap == 2 ? slider11 = 9000;
            
            );
            
            mouse_cap > 0 ? (
              mouseon == 0 ? (
                mouseclick = 1;
                mouse_cap == 2 ? val=10 : val = 0.5;
            //Q Low 1
            (mouse_x>=176&&mouse_x<=230&&mouse_y>=124&&mouse_y<=180) ? (mouseon=3;mouseclick=3;);
            
            //Gain Low 1
            (mouse_x>=245&&mouse_x<=298&&mouse_y>=124&&mouse_y<=180) ? (mouseon=4;mouseclick=4;);
            
            //Q Low/Mid 2
            (mouse_x>=325&&mouse_x<=380&&mouse_y>=124&&mouse_y<=180) ? (mouseon=6;mouseclick=6;);
            
            //Gain Low/Mid 2
            (mouse_x>=400&&mouse_x<=455&&mouse_y>=124&&mouse_y<=180) ? (mouseon=7;mouseclick=7;);
            
            
            //Q Mid/High 3
            (mouse_x>=478&&mouse_x<=536&&mouse_y>=124&&mouse_y<=180) ? (mouseon=9;mouseclick=9;);
            
            //Gain Mid/High 3
            (mouse_x>=552&&mouse_x<=610&&mouse_y>=124&&mouse_y<=180) ? (mouseon=10;mouseclick=10;);
            
            //Q High 4
            (mouse_x>=638&&mouse_x<=687&&mouse_y>=124&&mouse_y<=180) ? (mouseon=12;mouseclick=12;);
            
            //Gain High 4
            (mouse_x>=707&&mouse_x<=759&&mouse_y>=124&&mouse_y<=180) ? (mouseon=13;mouseclick=13;);
            
            //Output
            (mouse_x>=805&&mouse_x<=860&&mouse_y>=45&&mouse_y<=90) ? (mouseon=15;mouseclick=15;);
    
  
            
            );
            //Q Low 1
             mouseon == 3 ? (
              mouse_y < saveY ? slider3 = max(0.5,slider3-val);
              mouse_y > saveY ? slider3 = min(10.0,slider3+val);
              mouse_cap == 2 ? slider3 = 0.5;
              
              );
              //Q Low/mid 2
               mouseon == 6 ? (
                mouse_y < saveY ? slider6 = max(0.5,slider6-val);
                mouse_y > saveY ? slider6 = min(10.0,slider6+val);
                mouse_cap == 2 ? slider6 = 0.5;
                
                );
                //Q mid/high 3
                 mouseon == 9 ? (
                  mouse_y < saveY ? slider9 = max(0.5,slider9-val);
                  mouse_y > saveY ? slider9 = min(10.0,slider9+val);
                  mouse_cap == 2 ? slider9 = 0.5;
                  
                  );
                  //Q high 4
                   mouseon == 12 ? (
                    mouse_y < saveY ? slider12 = max(0.5,slider12-val);
                    mouse_y > saveY ? slider12 = min(10.0,slider12+val);
                    mouse_cap == 2 ? slider12 = 0.5;
                    
                    );
                    //Gain Low 1
                     mouseon == 4 ? (
                      mouse_y < saveY ? slider4 = max(-12,slider4-val);
                      mouse_y > saveY ? slider4 = min(6.0,slider4+val);
                      mouse_cap == 2 ? slider4 = 0;
                      
                      );
                      //Gain Low/mid 2
                       mouseon == 7 ? (
                        mouse_y < saveY ? slider7 = max(-12,slider7-val);
                        mouse_y > saveY ? slider7 = min(6.0,slider7+val);
                        mouse_cap == 2 ? slider7 = 0;
                        
                        );
                        //Gain mid/high 3
                         mouseon == 10 ? (
                          mouse_y < saveY ? slider10 = max(-12,slider10-val);
                          mouse_y > saveY ? slider10 = min(6.0,slider10+val);
                          mouse_cap == 2 ? slider10 = 0;
                          
                          );
                          //Gain high 4
                           mouseon == 13 ? (
                            mouse_y < saveY ? slider13 = max(-12,slider13-val);
                            mouse_y > saveY ? slider13 = min(6.0,slider13+val);
                            mouse_cap == 2 ? slider13 = 0;
                            
                            );
                            //output
                             mouseon == 15 ? (
                              mouse_y < saveY ? slider15 = max(-12,slider15-val);
                              mouse_y > saveY ? slider15 = min(12,slider15+val);
                              mouse_cap == 2 ? slider15 = 0.0;
                              );
                              
                    
                          //2
               );
     //3
                 
         );
    
         gain = 10^(slider15/20);
         
         a0 = 1;
         s0 = 1;
         q0 = 1 / (sqrt((a0 + 1/a0)*(1/s0 - 1) + 2));
         w00 = 2 * $pi * slider1/srate;
         cosw00 = cos(w00);
         sinw00 = sin(w00);
         alpha0 = sinw00 / (2 * q0);
         
         b00 = (1 + cosw00)/2;
         b10 = -(1 + cosw00);
